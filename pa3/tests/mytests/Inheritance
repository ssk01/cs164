print y.k
_____________________
def x = Object:new({})
x.print_three = lambda(self) {
    self.three = self.three - 3
}
def y = x:new({})
def k = y:new({three = 3})
k:print_three()
print k.three
_____________________
def x = Object:new({three = 3})
x.print_three = lambda(self) {
    self.three = self.three - 3
}
def k = x:new({})
print k.three
print x.three
k:print_three()
print k.three
print x.three
