#p4
def lays = Object:new({a = 5})
Object.eat = lambda(self) {
    self.a = self.a - 1
}
def alice = lays:new({})
print alice.a
alice:eat()
print alice.a
print '____________'
lays.eat = lambda(self) {
    self.a = self.a - 2
}
