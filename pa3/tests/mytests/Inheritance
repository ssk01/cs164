# p1
def food = Object:new({amount = 5})
food.eat = lambda() {
    food.amount = food.amount - 1
}
def f1 = food:new({})
print f1.amount
f1.eat()
print f1.amount
def f2 = food:new({})
print f2.amount
food = null
print f2.amount
f2.eat()
print f2.amount
