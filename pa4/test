def k(x, y) {
    yield(x+y)
}
def g(x, y) {
    def ck = coroutine(k)
    def h(a, b) {
        3 + yield(resume(ck, a, b))
    }
    h(x, y)
}
def f(x, y) {
    def cg = coroutine(g)
    resume(cg, x, y)
}
